[egg_info]
tag_build = dev
tag_svn_revision = true

[nosetests]
with-pylons=test.ini

# Babel configuration
[compile_catalog]
domain = abchat
directory = abchat/i18n
statistics = true

[extract_messages]
add_comments = TRANSLATORS:
output_file = abchat/i18n/abchat.pot
width = 80
keywords = l_

[init_catalog]
domain = abchat
input_file = abchat/i18n/abchat.pot
output_dir = abchat/i18n

[update_catalog]
domain = abchat
input_file = abchat/i18n/abchat.pot
output_dir = abchat/i18n
previous = true
